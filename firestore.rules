rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Allow read/write access to the tutor's data
    match /users/{userId} {
      allow read, write: if true;
    }

    // Allow read/write access to notified collection
    match /notified/{docId} {
      allow read, write: if true;
    }
  }
}
